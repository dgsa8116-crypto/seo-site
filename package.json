    "start": "next start"
  },
  "dependencies": {
    "next": "15.5.6",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "node-fetch": "^3.3.2",
    "next-sitemap": "^4.2.3",
    "sharp": "^0.33.4"
  }
}