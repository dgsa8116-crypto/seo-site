{
  "name": "seo-site-pro-tw",
  "version": "1.0.4",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "generate-meta": "node scripts/generate-meta.mjs",
    "compress-images": "node scripts/compress-images.mjs",
